<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.command_dispatcher_system.mapper.DepartmentMapper" >

    <!--全表查询部门-->
    <select id="selectAll" resultType="com.example.command_dispatcher_system.entity.Department">
        select did as id ,department_name as departmentName,
               department_desc as description
        from department_info
    </select>

<!--DAO层的东西-->
</mapper>